{
  "$schema": "https://raw.githubusercontent.com/jetpack-io/devbox/main/.schema/devbox.schema.json",
  "packages": {
    "pre-commit": "3.7.1",
    "yq-go": "4.44.1",
    "d2": "0.7.0",
    "github:rdeville-public/dotfiles-nixos-config#default": "",
    "path:.#default": ""
