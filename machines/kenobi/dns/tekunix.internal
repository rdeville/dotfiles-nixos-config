$ORIGIN tekunix.internal.
$TTL    1h
@                     IN SOA   ns0.fdn.fr ns1.fdn.fr (
                                  1  ; Serial
                                  1h ; Refresh
                                  1h ; Retry
                                  1h ; Expire
                                  1h ; Negative Cache TTL
                                )
                      IN NS    ns0.fdn.fr
                      IN NS    ns1.fdn.fr

; External servers
; -----------------------------------------------------------------------------
darth-plagueis        IN CNAME romaindeville.ovh
darth-vader           IN CNAME romaindeville.fr

; Router IP and Personal Machines
; -----------------------------------------------------------------------------
@                     IN A     172.30.0.1
kenobi                IN A     172.30.0.1
darth-maul            IN A     172.30.0.2
rey                   IN A     172.30.1.3

; DEV Cluster Network
; -----------------------------------------------------------------------------
*.dev                 IN A     172.30.160.201
vm-kenobi-k8s-dev     IN A     172.30.160.201 ; CP & Worker (VM)
vm-darth-maul-k8s-dev IN A     172.30.160.202 ; Worker (VM)
vm-rey-k8s-dev        IN A     172.30.160.203 ; Worker (VM)

; STG Cluster Network
; -----------------------------------------------------------------------------
*.stg                 IN A     172.30.144.201
vm-kenobi-k8s-stg     IN A     172.30.144.201 ; CP & Worker (VM)
; Control-Plane & Worker
luke                  IN A     172.30.144.11  ; CP & Worker
leia                  IN A     172.30.144.12  ; CP & Worker

; PRD Cluster Network
; -----------------------------------------------------------------------------
